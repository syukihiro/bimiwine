.header
  = link_to '/wines/index' do
    = image_tag 'IMG_7119.jpg', class: 'bimi-m'
.content2
  .content2-title
    %span プロフィール編集
= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f|
  = render "devise/shared/error_messages", resource: resource
  .field
    = f.label :NickName
    = f.text_field :nickname, placeholder: "テスト" ,autocomplete: "nickname"
  .field
    = f.label :email
    = f.email_field :email, autofocus: true, placeholder: "test@co.jp", autocomplete: "email"
  .actions
    = f.submit "Update"